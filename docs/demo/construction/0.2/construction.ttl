    rdfs:subClassOf <http://w3id.org/CEON/ontology/resourceODP/ResourceProperty> .

:MaximalDismantlingCostPerSqmValue a owl:Class ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty <http://qudt.org/schema/qudt/hasUnit> ;
                        owl:someValuesFrom <http://qudt.org/schema/qudt/Unit> ] [ a owl:Restriction ;
                        owl:allValuesFrom <http://qudt.org/schema/qudt/Unit> ;
