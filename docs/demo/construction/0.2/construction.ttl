dcterms:contributor a owl:AnnotationProperty .

dcterms:created a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

: a owl:Ontology ;
    dcterms:contributor "Eva Blomqvist",
        "Huanyu Li" ;
    dcterms:created "2025-03-18" ;
    dcterms:creator "Huanyu Li" ;
    dcterms:description "The construction use case ontology in Onto-DESIDE, as a module of CEON." ;
    dcterms:license "https://creativecommons.org/licenses/by/4.0/" ;
    dcterms:title "Circular Economy Ontology Network (CEON) - Construction Module" ;
    vann:preferredNamespacePrefix "construction" ;
    vann:preferredNamespaceUri "http://w3id.org/CEON/demo/construction/" ;
    rdfs:seeAlso <https://github.com/LiUSemWeb/CEON> ;
    owl:imports <http://w3id.org/CEON/ontology/actor/0.4/>,
        <http://w3id.org/CEON/ontology/material/0.4/>,
        <http://w3id.org/CEON/ontology/product/0.5/>,
        <http://w3id.org/CEON/ontology/quantity/0.1/> ;
    owl:versionIRI <http://w3id.org/CEON/demo/construction/0.2/> ;
    owl:versionInfo "0.2" .

:A1RawMAterialExtractionImpact a owl:Class ;
    rdfs:subClassOf :ProductStageImpact .

:A2TransportationOfRawMaterialsImpact a owl:Class ;
    rdfs:subClassOf :ProductStageImpact .

:A3ManufacturingImpact a owl:Class ;
    rdfs:subClassOf :ProductStageImpact .

:A4TransportationOfManufacturedProductsImpact a owl:Class ;
    rdfs:subClassOf :ProductStageImpact .

:A5TheConstructionPhaseImpact a owl:Class ;
    rdfs:subClassOf :ProductStageImpact .

:B1InstalledProductImpact a owl:Class ;
    rdfs:subClassOf :UseStageImpact .

:B2MaintenanceImpact a owl:Class ;
    rdfs:subClassOf :UseStageImpact .

:B3RepairImpact a owl:Class ;
    rdfs:subClassOf :UseStageImpact .

:B4ReplacementImpact a owl:Class ;
    rdfs:subClassOf :UseStageImpact .

:B5RefurbishmentImpact a owl:Class ;
    rdfs:subClassOf :UseStageImpact .

:B6OperationalEnergyConsumption a owl:Class ;
    rdfs:subClassOf :UseStageImpact .

:B7OperationalWaterUse a owl:Class ;
    rdfs:subClassOf :UseStageImpact .

:BeyondEndOfLife a owl:Class ;
