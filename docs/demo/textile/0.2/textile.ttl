    :CFmapping "CF157" ;
    rdfs:subClassOf :FinishingProcess .

:BatchOfTextileObjects a owl:Class ;
    rdfs:label "Batch of Textile Objects" ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty :hasColor ;
                        owl:someValuesFrom :Color ] [ a owl:Restriction ;
                        owl:allValuesFrom :Color ;
                        owl:onProperty :hasColor ] ) ],
        <http://w3id.org/CEON/ontology/resourceODP/BatchOfObjects> .

:BrandOfResource a owl:Class ;
    rdfs:label "Brand of resource" ;
    rdfs:comment "The relation where the role of the actor is the brand of a certain resource." ;
    rdfs:subClassOf actorODP:ResourceRelation ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( actorODP:ResourceRelation [ a owl:Restriction ;
                        owl:hasValue :brand ;
                        owl:onProperty actorODP:participantRole ] ) ] .

