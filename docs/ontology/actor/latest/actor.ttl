<http://w3id.org/CEON/ontology/actorODP/Participation> a owl:Class .

<http://w3id.org/CEON/ontology/actorODP/participatingResource> a owl:ObjectProperty .

<http://w3id.org/CEON/ontology/cvn/CVN> a owl:Class ;
    rdfs:label "Circular Value Network" ;
    rdfs:comment "A collaboration between actors that constitute a Circular Value Newtworks, implicitly or explicitly set up." ;
    rdfs:subClassOf <http://w3id.org/CEON/ontology/cvn/Collaboration> .

<http://w3id.org/CEON/ontology/cvn/Collaboration> a owl:Class .

<http://w3id.org/CEON/ontology/resourceODP/Resource> a owl:Class .

actor:ActorCollaborationRole a owl:Class ;
    rdfs:label "Actor collaboration role" ;
    rdfs:comment "The roles of an actor involved in a collaboration." ;
    rdfs:subClassOf <http://w3id.org/CEON/ontology/actorODP/Role> .

<http://w3id.org/CEON/ontology/actorODP/Capability> a owl:Class .

<http://w3id.org/CEON/ontology/actorODP/Role> a owl:Class .

<http://w3id.org/CEON/ontology/actorODP/capabilityProperty> a owl:ObjectProperty .

<http://w3id.org/CEON/ontology/actorODP/participationIn> a owl:ObjectProperty .

<http://w3id.org/CEON/ontology/actorODP/Actor> a owl:Class .

<http://w3id.org/CEON/ontology/actorODP/participatingActor> a owl:ObjectProperty .

<http://w3id.org/CEON/ontology/actorODP/CollaborationParticipation> a owl:Class .

actor:ActorCVNRole a owl:Class ;
    rdfs:label "Actor Circular Value Network role" ;
    rdfs:comment "The role(s) of an actor in a Circular Value Network, which is a specific type of collaboration." ;
    rdfs:subClassOf actor:ActorCollaborationRole .

actor:ActorResourceRole a owl:Class ;
    rdfs:label "Actor resource role" ;
    rdfs:comment "The role(s) of an actor in relation to a resource." ;
    rdfs:subClassOf <http://w3id.org/CEON/ontology/actorODP/Role> .

<http://w3id.org/CEON/ontology/actorODP/participantRole> a owl:ObjectProperty .

<http://w3id.org/CEON/ontology/actorODP/ResourceRelation> a owl:Class ;
    rdfs:label "Resource Relation" ;
    rdfs:comment "A relation between a resource, an actor, and the role the actor has in relation to the resource. For instance, the role of owner that a certain actor takes for a certain resource for a certain period of time. Or the seller of a certain resource, until that resource has been sold, and the actor no longer has that role in relation to the resource." ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( <http://w3id.org/CEON/ontology/actorODP/Participation> [ a owl:Restriction ;
                        owl:onProperty <http://w3id.org/CEON/ontology/actorODP/participantRole> ;
                        owl:someValuesFrom actor:ActorResourceRole ] [ a owl:Restriction ;
                        owl:onProperty <http://w3id.org/CEON/ontology/actorODP/participatingActor> ;
                        owl:someValuesFrom <http://w3id.org/CEON/ontology/actorODP/Actor> ] [ a owl:Restriction ;
                        owl:onProperty <http://w3id.org/CEON/ontology/actorODP/participatingResource> ;
                        owl:someValuesFrom <http://w3id.org/CEON/ontology/resourceODP/Resource> ] ) ] .

