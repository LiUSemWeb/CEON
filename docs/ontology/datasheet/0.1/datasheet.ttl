@prefix : <http://www.semanticweb.org/evabl45/ontologies/2023/8/untitled-ontology-86/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://w3id.org/CEON/ontology/datasheet/> a owl:Ontology ;
    owl:imports <http://w3id.org/CEON/ontology/resourceODP/0.2/>,
        <http://www.w3.org/ns/prov-o-20130430> ;
    owl:versionIRI <http://w3id.org/CEON/ontology/datasheet/0.1/> .

:DataSheet a owl:Class ;
    rdfs:label "Data Sheet" ;
    rdfs:comment "A data sheet is a collection of data about something, e.g. a set of facts about a product." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://w3id.org/CEON/ontology/resourceODP/containsInformation> ;
            owl:someValuesFrom :Statement ],
        <http://w3id.org/CEON/ontology/resourceODP/Information>,
        prov:Entity .

:isRealizationOf a owl:ObjectProperty ;
    rdfs:label "is realization of" ;
    rdfs:comment "The physical or digital realisation of a piece of information, e.g. a pdf file that stores the information about a product." ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :DigitalObject <http://w3id.org/CEON/ontology/resourceODP/PhysicalObject> ) ] ;
    rdfs:range <http://w3id.org/CEON/ontology/resourceODP/Information> .

:DigitalObject a owl:Class ;
    rdfs:label "Digital object" ;
    rdfs:comment "A digital object, e.g. file or other digital resource, exisiting on a computer, server, on the web etc." ;
    rdfs:subClassOf <http://w3id.org/CEON/ontology/resourceODP/Resource> .

:Statement a owl:Class ;
    rdfs:label "Statement" ;
    rdfs:comment "A single fact, or statement about something. Usually contained within some larger piece of information, e.g. a document." ;
    rdfs:subClassOf <http://w3id.org/CEON/ontology/resourceODP/Information> .

