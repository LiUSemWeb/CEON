@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odp: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix resourceODP: <http://w3id.org/CEON/ontology/resourceODP/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dc:description a owl:AnnotationProperty .

dcterms:contributor a owl:AnnotationProperty .

dcterms:created a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

resourceODP: a owl:Ontology ;
    dcterms:contributor "Eva Blomqvist",
        "Mikael Lindecrantz",
        "Robin Keskisärkkä" ;
    dcterms:created "2023-03-16"^^xsd:string ;
    dcterms:creator "Huanyu Li" ;
    dcterms:description "The Resource ODP." ;
    dcterms:license "https://github.com/LiUSemWeb/CEON/blob/master/LICENSE"^^xsd:string ;
    dcterms:title "Resource ODP"^^xsd:string ;
    vann:preferredNamespacePrefix "resourceODP"^^xsd:string ;
    vann:preferredNamespaceUri "http://w3id.org/CEON/ontology/0.1/resourceODP/"^^xsd:string ;
    odp:coversRequirements "Covers the following requirements from Onto-DESIDE D3.1: CVN-Resource-1,3, CVN-Composition-1,2, CVN-ResrouceType-4, C7-3, E2-2, E4-6, E5-1, E6-3, T3-1." ;
    rdfs:seeAlso "https://github.com/LiUSemWeb/CEON"^^xsd:string ;
    owl:versionIRI <http://w3id.org/CEON/ontology/0.1/resourceODP/> ;
    owl:versionInfo "0.1"^^xsd:string .

resourceODP:Information a owl:Class ;
    rdfs:label "Information"@en ;
    rdfs:seeAlso "http://emmo.info/emmo#EMMO_64c72d00_7582_44ea_a0b5_3a14e50acc36",
        "Information is an abstract concept that represents any kind of interpretations. For instance, information can be data generated by software systems or data used by people for communications."@en ;
    rdfs:subClassOf resourceODP:Resource .

resourceODP:PhysicalObject a owl:Class ;
    rdfs:label "PhysicalObject"@en ;
    rdfs:comment "A physical object is a collection of matter."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty resourceODP:hasConstituent ;
                        owl:someValuesFrom resourceODP:Constituent ] [ a owl:Restriction ;
                        owl:allValuesFrom resourceODP:Constituent ;
                        owl:onProperty resourceODP:hasConstituent ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty resourceODP:hasMatter ;
                        owl:someValuesFrom resourceODP:Matter ] [ a owl:Restriction ;
                        owl:allValuesFrom resourceODP:Matter ;
                        owl:onProperty resourceODP:hasMatter ] ) ],
        resourceODP:Resource .

resourceODP:Resource a owl:Class ;
    rdfs:label "Resource"@en ;
    rdfs:comment "A resource able to be handled in the context of a circular value network, e.g. data generated by software systems in the CVN, materials or prooducts as physical objects handled in the CVN."@en .

resourceODP:hasConstituent a owl:ObjectProperty ;
    rdfs:label "hasConstituent"@en ;
    rdfs:comment "hasConstitutent intends to represent that a physical object can have a collection of composing components."@en ;
    rdfs:domain resourceODP:PhysicalObject ;
    rdfs:range resourceODP:Constituent .

resourceODP:hasMatter a owl:ObjectProperty ;
    rdfs:label "hasMatter"@en ;
    rdfs:comment "hasMatter intends to represent that a physical object can have a collection of matter."@en ;
    rdfs:domain resourceODP:PhysicalObject ;
    rdfs:range resourceODP:Matter .

resourceODP:Constituent a owl:Class ;
    rdfs:label "Constituent"@en ;
    rdfs:comment "A constituent is a component of object."@en ;
    rdfs:seeAlso "http://emmo.info/emmo#EMMO_ceaaf9f7_fd11_424b_8fda_9afa186af186" .

resourceODP:Matter a owl:Class ;
    rdfs:label "Matter"@en ;
    rdfs:comment "A matter is a physical substance."@en ;
    rdfs:seeAlso "http://emmo.info/emmo#EMMO_5b2222df_4da6_442f_8244_96e9e45887d1" .

