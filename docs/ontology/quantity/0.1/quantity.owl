  <rdf:Description rdf:about="http://w3id.org/CEON/ontology/quantity#hasTestingCost">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://qudt.org/schema/qudt/quantityValue"/>
    <rdfs:comment>The cost of testing, expressed as an amount of money with a currency unit.</rdfs:comment>
    <rdfs:comment>has testing cost</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://qudt.org/vocab/unit/EURO-PER-M2">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://qudt.org/schema/qudt/Unit"/>
    <dcterms:description rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML">Euro Per Square Meter (€/km) is a unit used to represent the cost or value of resource per square meter.</dcterms:description>
    <qudt:symbol>€/m²</qudt:symbol>
    <rdfs:label xml:lang="en">Euro Per Square Meter</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://qudt.org/vocab/unit/EURO-PER-KiloM">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://qudt.org/schema/qudt/Unit"/>
    <dcterms:description rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML">Euro Per Kilometer (€/km) is a unit used to represent the cost or value associated with transporting a resource.</dcterms:description>
    <qudt:symbol>€/km</qudt:symbol>
    <rdfs:label xml:lang="en">Euro Per Kilometer</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://w3id.org/CEON/ontology/quantity/">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
