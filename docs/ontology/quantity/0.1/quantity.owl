    <dcterms:creator>Robin Keskisärkkä</dcterms:creator>
    <dcterms:license>https://creativecommons.org/licenses/by/4.0/</dcterms:license>
    <dcterms:title xml:lang="en">Circular Economy Ontology Network (CEON) - Quantity Module</dcterms:title>
    <vann:preferredNamespacePrefix>quantity</vann:preferredNamespacePrefix>
    <vann:preferredNamespaceUri>http://w3id.org/CEON/ontology/quantity/</vann:preferredNamespaceUri>
    <rdfs:comment xml:lang="en">This module defines base properties to describe quantitites with standardized units.</rdfs:comment>
    <rdfs:seeAlso>https://github.com/LiUSemWeb/Materials-Design-Ontology</rdfs:seeAlso>
    <owl:versionInfo xml:lang="en">0.1</owl:versionInfo>
  </rdf:Description>
