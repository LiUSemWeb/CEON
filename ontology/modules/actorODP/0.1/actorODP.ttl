#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/CEON/ontology/actorODP/actorType
actorODP:actorType rdf:type owl:ObjectProperty ;
                   rdfs:domain actorODP:Actor ;
                   rdfs:range actorODP:ActorType ;
                   rdfs:comment "The type of the actor." ;
                   rdfs:label "has actor type" .


###  http://w3id.org/CEON/ontology/actorODP/basedOnCapability
actorODP:basedOnCapability rdf:type owl:ObjectProperty ;
                           rdfs:domain actorODP:CollaborationParticipation ;
                           rdfs:range actorODP:Capability ;
                           rdfs:comment "An actor participates in a collaboration based on that it has some capability that is useful for the collaboration." ;
                           rdfs:label "based on capability" .


###  http://w3id.org/CEON/ontology/actorODP/capabilityOf
actorODP:capabilityOf rdf:type owl:ObjectProperty ;
                      rdfs:domain actorODP:Capability ;
                      rdfs:range actorODP:Actor ;
                      rdfs:label "Relates to the actor holding the capability." ,
                                 "capability of" .


###  http://w3id.org/CEON/ontology/actorODP/capabilityProperty
actorODP:capabilityProperty rdf:type owl:ObjectProperty ;
                            rdfs:domain actorODP:Capability ;
                            rdfs:comment "Relates some properties to the capability, such as the parameters of it, or the needed resources." ;
                            rdfs:label "capability property" .


###  http://w3id.org/CEON/ontology/actorODP/hasLocation
actorODP:hasLocation rdf:type owl:ObjectProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( actorODP:Actor
                                                 <http://w3id.org/CEON/ontology/resourceODP/Resource>
                                               )
                                 ] .


###  http://w3id.org/CEON/ontology/actorODP/participantRole
actorODP:participantRole rdf:type owl:ObjectProperty ;
                         rdfs:domain actorODP:Participation ;
                         rdfs:range actorODP:Role ;
                         rdfs:comment "Holds the value of the role of the participant in this participation relation." ;
                         rdfs:label "has participant role" .


###  http://w3id.org/CEON/ontology/actorODP/participatingActor
actorODP:participatingActor rdf:type owl:ObjectProperty ;
                            rdfs:domain actorODP:Participation ;
                            rdfs:range actorODP:Actor ;
                            rdfs:comment "Holds the value of the actor involved in this participation relation." ;
                            rdfs:label "has participating actor" .


###  http://w3id.org/CEON/ontology/actorODP/participatingResource
actorODP:participatingResource rdf:type owl:ObjectProperty ;
                               rdfs:domain actorODP:ResourceRelation ;
                               rdfs:range <http://w3id.org/CEON/ontology/resourceODP/Resource> ;
                               rdfs:comment "The resource that this participation relation relates to, i.e. for which the actor holds the specified role." ;
                               rdfs:label "has participating resource" .


###  http://w3id.org/CEON/ontology/actorODP/participationIn
actorODP:participationIn rdf:type owl:ObjectProperty ;
                         rdfs:domain actorODP:CollaborationParticipation ;
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( <http://w3id.org/CEON/ontology/cvn/Collaboration>
                                                    <http://w3id.org/CEON/ontology/process/Process>
                                                  )
                                    ] ;
                         rdfs:comment "The collaboration or process that this participation relates to." ;
                         rdfs:label "is a participation in" .


