    <!-- http://w3id.org/CEON/ontology/product/MatterComposition -->

    <owl:Class rdf:about="http://w3id.org/CEON/ontology/product/MatterComposition">
        <rdfs:subClassOf rdf:resource="http://w3id.org/CEON/ontology/resourceODP/Composition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/CEON/ontology/product/associatedWithMatter"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://w3id.org/CEON/ontology/resourceODP/Matter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A product composition is the composed information of a product in terms of a specific component, in which such a component is also a matter.</rdfs:comment>
        <rdfs:label xml:lang="en">MatterComposition</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/CEON/ontology/product/Product -->

    <owl:Class rdf:about="http://w3id.org/CEON/ontology/product/Product">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/CEON/ontology/product/hasComposition"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/CEON/ontology/product/MatterComposition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/CEON/ontology/product/hasComposition"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/CEON/ontology/product/ProductComposition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A product model is a model that is used for manufacturing products.</rdfs:comment>
        <rdfs:label xml:lang="en">Product Model</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/CEON/ontology/product/ProductComposition -->

    <owl:Class rdf:about="http://w3id.org/CEON/ontology/product/ProductComposition">
        <rdfs:subClassOf rdf:resource="http://w3id.org/CEON/ontology/resourceODP/Composition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/CEON/ontology/product/associatedWithProductModel"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://w3id.org/CEON/ontology/product/Product"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A product composition is the composed information of a product in terms of a specific component, in which such a component is also a product.</rdfs:comment>
        <rdfs:label xml:lang="en">ProductComposition</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/CEON/ontology/product/ProductObject -->

    <owl:Class rdf:about="http://w3id.org/CEON/ontology/product/ProductObject">
        <rdfs:subClassOf rdf:resource="http://w3id.org/CEON/ontology/resourceODP/PhysicalObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/CEON/ontology/product/modelledBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://w3id.org/CEON/ontology/product/Product"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A product object is a physical object put into a market for sale. The product can be a thing that grows naturally or produced through some chemical or manufacturing processes.</rdfs:comment>
        <rdfs:label xml:lang="en">ProductObject</rdfs:label>
    </owl:Class>
    


