@prefix : <http://w3id.org/CEON/ontology/textile/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://w3id.org/CEON/ontology/textile/> .

<http://w3id.org/CEON/demo/textile/> rdf:type owl:Ontology ;
                                      owl:versionIRI <http://w3id.org/CEON/demo/textile/0.1/> ;
                                      owl:imports <http://w3id.org/CEON/ontology/actorODP/0.2/> ,
                                                  <http://w3id.org/CEON/ontology/datasheet/0.1/> ,
                                                  <http://w3id.org/CEON/ontology/product/0.2/> ,
                                                  <http://w3id.org/CEON/ontology/provenance/0.1/> ,
                                                  <http://w3id.org/CEON/ontology/quantity/0.1/> ;
                                      <http://purl.org/dc/terms/contributor> "Huanyu Li" ,
                                                                             "Mikael Lindecrantz" ,
                                                                             "Robin Keskisärkkä" ;
                                      <http://purl.org/dc/terms/creator> "Eva Blomqvist" ;
                                      <http://purl.org/dc/terms/description> "The Textile module of CEON (Circular Economy Ontology Network)." ;
                                      <http://purl.org/dc/terms/license> "https://creativecommons.org/licenses/by/4.0/" ;
                                      <http://purl.org/dc/terms/title> "Circular Economy Ontology Network (CEON) - Textile Module" ;
                                      <http://purl.org/vocab/vann/preferredNamespaceUri> "http://w3id.org/CEON/demo/textile/" ;
                                      <http://qudt.org/schema/qudt/prefix> "textile" ;
                                      rdfs:seeAlso "https://github.com/LiUSemWeb/CEON" ;
                                      owl:versionInfo 0.1 .

#################################################################
#    Annotation properties
#################################################################

###  http://w3id.org/CEON/demo/textile/CFmapping
:CFmapping rdf:type owl:AnnotationProperty .


###  http://w3id.org/CEON/demo/textile/PCDSmapping
:PCDSmapping rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/CEON/demo/textile/anticipatedLeakageDuringUse
:anticipatedLeakageDuringUse rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :hasProperty ;
                             :PCDSmapping "5040-5046" ;
                             rdfs:comment "Percentage of the product content anticipated to become leakage during the use phase due to for example wear & tear, oxidation, erosion, etc."@en .


###  http://w3id.org/CEON/demo/textile/chemicalSubstanceDisclosureThreshold
:chemicalSubstanceDisclosureThreshold rdf:type owl:ObjectProperty ;
                                      rdfs:range :TextileProductCompositionDisclosureStatement .


###  http://w3id.org/CEON/demo/textile/designedForRecyclingAtSameQuality
:designedForRecyclingAtSameQuality rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf :hasProperty ;
                                   rdfs:range :TextileProductDesignForRecycling ;
                                   :PCDSmapping "5030-5037" ;
                                   rdfs:comment "Percentage of the product that is designed for recycling at the same level of quality. The remainder of the materials is foreseen by the manufacturer to be recycled at a lower quality than the original content."@en ;
                                   rdfs:label "desinged for recycling at the same level of quality"@en .


###  http://w3id.org/CEON/demo/textile/disclosureValue
:disclosureValue rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://w3id.org/CEON/ontology/quantity#objectValue> ;
                 rdfs:domain :TextileChemicalSubstanceThresholdStatement ;
                 rdfs:range :AmountThreshold .


###  http://w3id.org/CEON/demo/textile/hasColor
:hasColor rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( <http://w3id.org/CEON/ontology/product/Product>
                                      <http://w3id.org/CEON/ontology/product/ProductObject>
                                      <http://w3id.org/CEON/ontology/resourceODP/BatchOfObjects>
                                    )
                      ] ;
          rdfs:range :Color .


###  http://w3id.org/CEON/demo/textile/hasDyeMethod
:hasDyeMethod rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasTextileManufacturingProcess ;
              rdfs:range :DyeingProcess ;
              :CFmapping "CF700" ;
              rdfs:label "Dye Method"@en .


###  http://w3id.org/CEON/demo/textile/hasDyeStuff
:hasDyeStuff rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasProperty ;
             :CFmapping "CF701" ;
             rdfs:label "Dye Stuff"@en .


###  http://w3id.org/CEON/demo/textile/hasFinishing
:hasFinishing rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasProperty ;
              rdfs:range :TextileFinishing .


###  http://w3id.org/CEON/demo/textile/hasFinishingStep
:hasFinishingStep rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasTextileManufacturingProcess ;
                  :CFmapping "CF704" ;
                  rdfs:label "Finishing Step"@en .


###  http://w3id.org/CEON/demo/textile/hasPostConsumerRecycledContentAmount
:hasPostConsumerRecycledContentAmount rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf <http://w3id.org/CEON/ontology/product/hasComposition> .


###  http://w3id.org/CEON/demo/textile/hasPreConsumerRecycledContentAmount
:hasPreConsumerRecycledContentAmount rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf <http://w3id.org/CEON/ontology/product/hasComposition> .


###  http://w3id.org/CEON/demo/textile/hasPrintFinishing
:hasPrintFinishing rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasProperty ;
                   rdfs:label "Print Finishing"@en .


###  http://w3id.org/CEON/demo/textile/hasPrintMethod
:hasPrintMethod rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasTextileManufacturingProcess ;
                rdfs:range :PrintingProcess ;
                :CFmapping "CF702" ;
                rdfs:label "Print Method"@en .


###  http://w3id.org/CEON/demo/textile/hasPrintStuff
:hasPrintStuff rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasProperty ;
               :CFmapping "CF703" ;
               rdfs:label "Print Stuff"@en .


###  http://w3id.org/CEON/demo/textile/hasProperty
:hasProperty rdf:type owl:ObjectProperty ;
             rdfs:domain <http://w3id.org/CEON/ontology/datasheet/DataSheet> ;
             rdfs:range <http://w3id.org/CEON/ontology/provenance/Statement> .


###  http://w3id.org/CEON/demo/textile/hasTextileManufacturingProcess
:hasTextileManufacturingProcess rdf:type owl:ObjectProperty ;
                                rdfs:range :TextileManufacturingProcess .


###  http://w3id.org/CEON/demo/textile/hasTextileProductType
:hasTextileProductType rdf:type owl:ObjectProperty ;
                       rdfs:domain :TextileProduct ;
                       rdfs:range :TextileProductType .


###  http://w3id.org/CEON/demo/textile/hasWaterProperty
:hasWaterProperty rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasProperty ;
                  rdfs:range :WaterProperty ;
                  :CFmapping "CF706" ;
                  rdfs:label "Water Property"@en .


###  http://w3id.org/CEON/demo/textile/productCompositionDisclosureAvailability
:productCompositionDisclosureAvailability rdf:type owl:ObjectProperty ;
                                          rdfs:comment "A product composition disclosed at the defined threshold is publicly available or a product composition disclosed at the defined threshold is available to the customer under secrecy agreement." .


###  http://w3id.org/CEON/demo/textile/productCompositionDisclosureValidation
:productCompositionDisclosureValidation rdf:type owl:ObjectProperty ;
                                        rdfs:comment "The product composition disclosed at the defined threshold has been validated by a third party or the product has been awarded an independent certification or standard related to its composition." .


###  http://w3id.org/CEON/demo/textile/recyclingAtSameQualityRange
:recyclingAtSameQualityRange rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf <http://w3id.org/CEON/ontology/quantity#objectValue> .


###  http://w3id.org/CEON/demo/textile/textileProductDesign
:textileProductDesign rdf:type owl:ObjectProperty .


###  http://w3id.org/CEON/demo/textile/textileProductDesignForRecycling
:textileProductDesignForRecycling rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf :textileProductDesign ;
                                  rdfs:range :TextileRecyclingProcess ;
                                  :PCDSmapping "5070, 5080, 5081, 5090, 5091" ;
                                  rdfs:comment "Product designed for a certain recycling method."@en .


###  http://w3id.org/CEON/demo/textile/weightFractionOfDisclosedSubstances
:weightFractionOfDisclosedSubstances rdf:type owl:ObjectProperty ;
                                     rdfs:comment "The weight fraction of all disclosed chemical substances is 0%, 0-10%, 10-25%, 25-50%, 50-75%, 75-95%, 95-99%, over 99%." .


#################################################################
#    Data properties
#################################################################

###  http://w3id.org/CEON/demo/textile/CEmark
:CEmark rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :certification ;
        rdfs:range xsd:boolean ;
        :PCDSmapping "5001" ;
        rdfs:comment "The product has theCE mark."@en .


###  http://w3id.org/CEON/demo/textile/availableInformation
:availableInformation rdf:type owl:DatatypeProperty .


###  http://w3id.org/CEON/demo/textile/biodegradabilityTestAvailable
:biodegradabilityTestAvailable rdf:type owl:DatatypeProperty ;
                               rdfs:subPropertyOf :availableInformation ;
                               rdfs:range xsd:boolean ;
                               :CFmapping "CF600" ;
                               rdfs:comment "Biodegradability test is available."@en .


###  http://w3id.org/CEON/demo/textile/certification
:certification rdf:type owl:DatatypeProperty .


###  http://w3id.org/CEON/demo/textile/chemicalCompliance
:chemicalCompliance rdf:type owl:DatatypeProperty ;
                    :CFmapping "CF707" .


###  http://w3id.org/CEON/demo/textile/collectionParameter
:collectionParameter rdf:type owl:DatatypeProperty .


###  http://w3id.org/CEON/demo/textile/collectionSystem
:collectionSystem rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :collectionParameter ;
                  rdfs:range xsd:boolean ;
                  :PCDSmapping "5050" ;
                  rdfs:comment "The manufacturer/ industry association has a dedicated collection system in place to gather and deliver products for recycling."@en .


###  http://w3id.org/CEON/demo/textile/designForProperties
:designForProperties rdf:type owl:DatatypeProperty .


###  http://w3id.org/CEON/demo/textile/designForRecyclingAmounts
:designForRecyclingAmounts rdf:type owl:DatatypeProperty .


###  http://w3id.org/CEON/demo/textile/designedForRecycling
:designedForRecycling rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf :designForProperties ;
                      rdfs:comment "The product is designed for recycling?e"@en .


###  http://w3id.org/CEON/demo/textile/designedForRefurbishment
:designedForRefurbishment rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf :designForProperties ;
                          :PCDSmapping "5010" ;
                          rdfs:comment "The product is desinged for refurbishment?"@en .


###  http://w3id.org/CEON/demo/textile/designedForRemanufacturing
:designedForRemanufacturing rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf :designForProperties ;
                            :PCDSmapping "5020" ;
                            rdfs:comment "The product is designed for remanufacturing?"@en .


###  http://w3id.org/CEON/demo/textile/designedForReuse
:designedForReuse rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :designForProperties ;
                  rdfs:comment "The product is designed for reused?" .


###  http://w3id.org/CEON/demo/textile/designedForReuseWMinimalMod
:designedForReuseWMinimalMod rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf :designedForReuse ;
                             :PCDSmapping "5000" ;
                             rdfs:comment "The product is designed for re-use as-is or with minimal modification."@en .


###  http://w3id.org/CEON/demo/textile/emissionByDesign
:emissionByDesign rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :designForRecyclingAmounts ;
                  :PCDSmapping "5060" ;
                  rdfs:comment "The portion of the product known to be emitted into the environment during use is designed for that purpose.  "@en .


###  http://w3id.org/CEON/demo/textile/finishingSteps
:finishingSteps rdf:type owl:DatatypeProperty ;
                :CFmapping "CF705" .


###  http://w3id.org/CEON/demo/textile/materialSafetyDataSheetAvailable
:materialSafetyDataSheetAvailable rdf:type owl:DatatypeProperty ;
                                  rdfs:subPropertyOf :availableInformation ;
                                  rdfs:range xsd:boolean ;
                                  :CFmapping "CF603" ;
                                  rdfs:comment "Material Safety Data Sheet for the product is available."@en .


###  http://w3id.org/CEON/demo/textile/materialSafetyDataSheetForDyeingAvailable
:materialSafetyDataSheetForDyeingAvailable rdf:type owl:DatatypeProperty ;
                                           rdfs:subPropertyOf :availableInformation ;
                                           rdfs:range xsd:boolean ;
                                           :CFmapping "CF602" ;
                                           rdfs:comment "Material Safety Data Sheets for Dyeing are available."@en .


###  http://w3id.org/CEON/demo/textile/materialSafetyDataSheetForFinishingAvailable
:materialSafetyDataSheetForFinishingAvailable rdf:type owl:DatatypeProperty ;
                                              rdfs:subPropertyOf :availableInformation ;
                                              rdfs:range xsd:boolean ;
                                              :CFmapping "CF601" ;
                                              rdfs:comment "Material Safety Data Sheets for Finishing are available."@en .


###  http://w3id.org/CEON/demo/textile/technicalDataSheetAvailable
:technicalDataSheetAvailable rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf :availableInformation ;
                             rdfs:range xsd:boolean ;
                             :CFmapping "CF604" ;
                             rdfs:comment "The Technical Data Sheet is available."@en .


#################################################################
#    Classes
#################################################################

###  http://w3id.org/CEON/demo/textile/AmountIntervalValue
:AmountIntervalValue rdf:type owl:Class .


###  http://w3id.org/CEON/demo/textile/AmountThreshold
:AmountThreshold rdf:type owl:Class .


###  http://w3id.org/CEON/demo/textile/Availability
:Availability rdf:type owl:Class .


###  http://w3id.org/CEON/demo/textile/BatchOfTextileObjects
:BatchOfTextileObjects rdf:type owl:Class ;
                       rdfs:subClassOf <http://w3id.org/CEON/ontology/resourceODP/BatchOfObjects> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasColor ;
                                         owl:someValuesFrom :Color
                                       ] .


###  http://w3id.org/CEON/demo/textile/ChemicalFinishing
:ChemicalFinishing rdf:type owl:Class ;
                   rdfs:subClassOf :TextileFinishing .


###  http://w3id.org/CEON/demo/textile/Color
:Color rdf:type owl:Class .


###  http://w3id.org/CEON/demo/textile/Dye
:Dye rdf:type owl:Class ;
     rdfs:subClassOf :TextileProperty .


###  http://w3id.org/CEON/demo/textile/DyeingProcess
:DyeingProcess rdf:type owl:Class ;
               rdfs:subClassOf :TextileManufacturingProcess ;
               rdfs:label "dyeing process" .


###  http://w3id.org/CEON/demo/textile/LeakageAmountInterval
:LeakageAmountInterval rdf:type owl:Class ;
                       rdfs:subClassOf :AmountIntervalValue .


###  http://w3id.org/CEON/demo/textile/MechanicalFinishing
:MechanicalFinishing rdf:type owl:Class ;
                     rdfs:subClassOf :TextileFinishing .


###  http://w3id.org/CEON/demo/textile/PostConsumerRecycledContentInterval
:PostConsumerRecycledContentInterval rdf:type owl:Class ;
                                     rdfs:subClassOf :AmountIntervalValue .


###  http://w3id.org/CEON/demo/textile/PreConsumerRecycledContentInterval
:PreConsumerRecycledContentInterval rdf:type owl:Class ;
                                    rdfs:subClassOf :AmountIntervalValue .


###  http://w3id.org/CEON/demo/textile/Print
:Print rdf:type owl:Class ;
       rdfs:subClassOf <http://w3id.org/CEON/ontology/resourceODP/Constituent> .


###  http://w3id.org/CEON/demo/textile/PrintStuff
:PrintStuff rdf:type owl:Class ;
            rdfs:subClassOf <http://w3id.org/CEON/ontology/resourceODP/Matter> .


###  http://w3id.org/CEON/demo/textile/PrintingProcess
:PrintingProcess rdf:type owl:Class ;
                 rdfs:subClassOf :TextileManufacturingProcess .


###  http://w3id.org/CEON/demo/textile/RecyclingAtSameLevelAmountInterval
:RecyclingAtSameLevelAmountInterval rdf:type owl:Class ;
                                    rdfs:subClassOf :AmountIntervalValue .


###  http://w3id.org/CEON/demo/textile/TextileChemicalSubstanceThresholdStatement
:TextileChemicalSubstanceThresholdStatement rdf:type owl:Class ;
                                            rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileDataSheet
:TextileDataSheet rdf:type owl:Class ;
                  rdfs:subClassOf <http://w3id.org/CEON/ontology/datasheet/DataSheet> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasProperty ;
                                    owl:someValuesFrom :TextileDataSheetStatement
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://w3id.org/CEON/ontology/resourceODP/isAbout> ;
                                    owl:someValuesFrom :TextileProduct
                                  ] .


###  http://w3id.org/CEON/demo/textile/TextileDataSheetIssuingStatement
:TextileDataSheetIssuingStatement rdf:type owl:Class ;
                                  rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileDataSheetStatement
:TextileDataSheetStatement rdf:type owl:Class ;
                           rdfs:subClassOf <http://w3id.org/CEON/ontology/provenance/Statement> .


###  http://w3id.org/CEON/demo/textile/TextileDataSheetUpdateStatement
:TextileDataSheetUpdateStatement rdf:type owl:Class ;
                                 rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileFinishing
:TextileFinishing rdf:type owl:Class ;
                  rdfs:subClassOf :TextileProperty .


###  http://w3id.org/CEON/demo/textile/TextileManufacturerStatement
:TextileManufacturerStatement rdf:type owl:Class ;
                              rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileManufacturingProcess
:TextileManufacturingProcess rdf:type owl:Class .


###  http://w3id.org/CEON/demo/textile/TextileManufacturingSiteStatement
:TextileManufacturingSiteStatement rdf:type owl:Class ;
                                   rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProduct
:TextileProduct rdf:type owl:Class ;
                rdfs:subClassOf <http://w3id.org/CEON/ontology/product/Product> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasColor ;
                                  owl:someValuesFrom :Color
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTextileProductType ;
                                  owl:someValuesFrom :TextileProductType
                                ] .


###  http://w3id.org/CEON/demo/textile/TextileProductCertification
:TextileProductCertification rdf:type owl:Class ;
                             rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductComponentRelation
:TextileProductComponentRelation rdf:type owl:Class ;
                                 rdfs:subClassOf <http://qudt.org/schema/qudt/Quantity> ,
                                                 :TextileDataSheetStatement ,
                                                 <http://w3id.org/CEON/ontology/actorODP/ResourceParticipation> ,
                                                 [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://qudt.org/schema/qudt/hasUnit> ;
                                                                          owl:someValuesFrom <http://qudt.org/schema/qudt/Unit>
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://w3id.org/CEON/ontology/actorODP/participatingObject> ;
                                                                          owl:someValuesFrom <http://w3id.org/CEON/ontology/resourceODP/Constituent>
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://w3id.org/CEON/ontology/actorODP/participatingSubject> ;
                                                                          owl:someValuesFrom :TextileProduct
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://qudt.org/schema/qudt/numericalValue> ;
                                                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                               owl:unionOf ( xsd:double
                                                                                                             xsd:integer
                                                                                                           )
                                                                                             ]
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ] .


###  http://w3id.org/CEON/demo/textile/TextileProductCompositionDisclosureStatement
:TextileProductCompositionDisclosureStatement rdf:type owl:Class ;
                                              rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductCompositionOfConcern
:TextileProductCompositionOfConcern rdf:type owl:Class ;
                                    rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductCompositionWeightFraction
:TextileProductCompositionWeightFraction rdf:type owl:Class ;
                                         rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductDataSheet
:TextileProductDataSheet rdf:type owl:Class ;
                         rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductDesignForDisassembly
:TextileProductDesignForDisassembly rdf:type owl:Class ;
                                    rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductDesignForRecycling
:TextileProductDesignForRecycling rdf:type owl:Class ;
                                  rdfs:subClassOf :TextileDataSheetStatement ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :recyclingAtSameQualityRange ;
                                                    owl:someValuesFrom :RecyclingAtSameLevelAmountInterval
                                                  ] .


###  http://w3id.org/CEON/demo/textile/TextileProductDesignForRefurbishment
:TextileProductDesignForRefurbishment rdf:type owl:Class ;
                                      rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductDesignForRemanufacturing
:TextileProductDesignForRemanufacturing rdf:type owl:Class ;
                                        rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductDesignForReuseMethod
:TextileProductDesignForReuseMethod rdf:type owl:Class ;
                                    rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductDesignToBeCleanlyRemoved
:TextileProductDesignToBeCleanlyRemoved rdf:type owl:Class ;
                                        rdfs:subClassOf :TextileProductDesignForDisassembly .


###  http://w3id.org/CEON/demo/textile/TextileProductDesignToBeDismantled
:TextileProductDesignToBeDismantled rdf:type owl:Class ;
                                    rdfs:subClassOf :TextileProductDesignForDisassembly .


###  http://w3id.org/CEON/demo/textile/TextileProductDesignedForBetterUse
:TextileProductDesignedForBetterUse rdf:type owl:Class ;
                                    rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductManufacturingMethod
:TextileProductManufacturingMethod rdf:type owl:Class ;
                                   rdfs:subClassOf :TextileDataSheetStatement .


###  http://w3id.org/CEON/demo/textile/TextileProductMatterCompositionRelation
:TextileProductMatterCompositionRelation rdf:type owl:Class ;
                                         rdfs:subClassOf <http://qudt.org/schema/qudt/Quantity> ,
                                                         :TextileDataSheetStatement ,
                                                         <http://w3id.org/CEON/ontology/actorODP/ResourceParticipation> ,
                                                         [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://w3id.org/CEON/ontology/actorODP/participatingObject> ;
                                                                                  owl:someValuesFrom <http://w3id.org/CEON/ontology/resourceODP/Matter>
                                                                                ]
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://w3id.org/CEON/ontology/actorODP/participatingSubject> ;
                                                                                  owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                       owl:unionOf ( :TextileProduct
                                                                                                                     <http://w3id.org/CEON/ontology/resourceODP/Constituent>
                                                                                                                   )
                                                                                                     ]
                                                                                ]
                                                                              ) ;
